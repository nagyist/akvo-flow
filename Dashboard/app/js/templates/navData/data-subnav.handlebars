    {{/view}}
     {{#if FLOW.Env.showStatisticsFeature}} 
      {{#view view.NavItemView item="manageAttributes" }}
        <a {{action doManageAttributes}}>{{t _manage_attributes}}</a>
      {{/view}}
    {{/if}}
    {{#view view.NavItemView item="bulkUpload" }}
    <a {{action doBulkUpload}}>{{t _bulk_upload_data}}</a>
